{
  "name": "alderiate-live",
  "description": "Extension navigateur pour le streamer Alderiate",
  "version": null,
  "manifest_version": 2,
  "permissions": [
    "notifications",
    "https://alderiate.com/"
  ],
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "Alderiate Live",
    "default_icon": {
      "48": "icons/icon_128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
