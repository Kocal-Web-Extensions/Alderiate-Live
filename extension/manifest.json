{
  "name": "Alderiate Live",
  "description": "Notifie si le streamer Alderiate est en train de stream sur Twitch.",
  "version": "0.1.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/alderiate_48.png",
    "128": "icons/alderiate_128.png"
  },
  "permissions": [
    "notifications",
    "https://api.twitch.tv/"
  ],
  "browser_action": {
    "default_title": "Alderiate Live",
    "default_icon": {
      "48": "icons/alderiate_48.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
